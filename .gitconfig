# everything below here stolen from https://github.com/garybernhardt/dotfiles/blob/master/.gitconfig
[diff]
[color]
    ui = auto
# some of these are suggestions from http://gitimmersion.com/lab_11.html
[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    dc = diff --cached
    amend = commit --amend
    aa = add --all
    head = !git l -1
    h = !git head
    hp = "!source ~/.githelpers && show_git_head"
    r = !git l -20
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    l = "!source ~/.githelpers && pretty_git_log"
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    la = !git l --all
    div = divergence
    gn = goodness
    gnc = goodness --cached
    fa = fetch --all
    pom = push origin master
    b = branch
    br = branch
    ds = diff --stat=160,120
    dh1 = diff HEAD~1
    type = cat-file -t
    dump = cat-file -p
    pushall = push --recurse-submodules=on-demand
[merge]
    tool = vim
[push]
	default = simple
[branch]
	autosetuprebase = always
